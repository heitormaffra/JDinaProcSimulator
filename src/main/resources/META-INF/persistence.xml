<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <persistence-unit name="persistence" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>br.cesjf.model.entities.Atividade</class>
    <class>br.cesjf.model.entities.Desenvolvedor</class>
    <class>br.cesjf.model.entities.Projeto</class>
    <class>backing.bean.Desenvolvedor</class>
    <class>backing.bean.Projeto</class>
    <class>backing.bean.Atividade</class>
    <class>model.entities.Atividade</class>
    <class>model.entities.Desenvolvedor</class>
    <class>model.entities.Projeto</class>
    <class>br.cesjf.model.entities.Equipe</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/dinaprocsimu"/>
      <property name="javax.persistence.jdbc.password" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
    </properties>
  </persistence-unit>
</persistence>
